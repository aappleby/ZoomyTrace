  "version": "0.2.0",
  "configurations": [
    {
      "name": "zoomytrace",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/build/ZoomyTrace",
      "logging": {
        "moduleLoad": false,
        "trace": true
      },
      "preLaunchTask": "build_zoomytrace",
    },
    {
      "name": "logicdump",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/logicdump/build/logicdump/main",
      "logging": {
        "moduleLoad": false,
        "trace": true
      },
      "preLaunchTask": "build_logicdump",
    }
  ]
}
