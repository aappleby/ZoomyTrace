{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build_zoomytrace",
      "type": "shell",
      "command": "hancho",
      "problemMatcher": ["$gcc"],
      "options":{ "cwd": "${workspaceFolder}" },
      "group": { "kind": "build", /*"isDefault": true*/ }
    },
    {
      "label": "build_logicdump",
      "type": "shell",
      "command": "hancho",
      "problemMatcher": ["$gcc"],
      "options":{ "cwd": "${workspaceFolder}/logicdump" },
      "group": { "kind": "build", /*"isDefault": true*/ }
    }
  ]
}
